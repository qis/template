{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "official",
  "vcpkg-configuration": {
    "default-registry": {
      "kind": "git",
      "repository": "git@github.com:qis/vcpkg",
      "baseline": "dd3097e305afa53f7b4312371f62058d2e665320"
    },
    "registries": [
      {
        "kind": "git",
        "repository": "git@github.com:qis/ports",
        "baseline": "ef92dc7116b4f55ffbd87c7004234b94d9d44921",
        "packages": [ "spdlog", "vcpkg-boost" ]
      }
    ],
    "overlay-ports": [ "./res/ports" ],
    "overlay-triplets": [ "./res/triplets" ]
  },
  "dependencies": [
    "boost-core",
    "boost-bimap",
    "boost-container",
    "boost-circular-buffer",
    "boost-lockfree",
    "boost-outcome",
    "boost-heap",
    "boost-leaf",
    "boost-asio",
    "boost-beast",
    "boost-json",
    "boost-url",
    "boost-filesystem",
    "boost-process",
    "boost-program-options",
    {
      "name": "spdlog",
      "default-features": false,
      "features": [
        "wchar"
      ]
    },
    {
      "name": "gtest",
      "features": [
        "cxx17"
      ]
    },
    "benchmark"
  ]
}
